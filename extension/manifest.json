{
  "manifest_version": 2,

  "name": "ZLND",
  "description": "Zlnd search.",
  "version": "1.0",
  "options_page": "options.html",

  "browser_action": {
   "default_icon": "img/icon.png",
   "default_popup": "popup.html"
  },
  "permissions": [
   "activeTab",
    "storage"
   ],
     "content_scripts": [ {
    "js": [ "lib/jquery.js", "js/popup.js" ],
    "matches": [ "http://*/*", "https://*/*"]
  }]
}
